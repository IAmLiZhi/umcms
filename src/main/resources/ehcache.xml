<?xml version="1.0" encoding="UTF-8"?>
<ehcache  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd">
	<diskStore path="/home/data/cache"/>
	<defaultCache maxElementsInMemory="10000" eternal="true" timeToIdleSeconds="1800" timeToLiveSeconds="1800"
		overflowToDisk="true" diskSpoolBufferSizeMB="3000" maxElementsOnDisk="100000000" 
		diskPersistent="true" diskExpiryThreadIntervalSeconds="120"/>
    
</ehcache>
